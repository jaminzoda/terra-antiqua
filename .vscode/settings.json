{
    "copyWatcher.sections": [
        {
            "source": "core",
            "destination": "core/test",
            "destinationRequired": true,
            "includes": [
                "**/*.js",
                "**/*.ts",
                "**/*.py"
            ],
            "excludes": [
                "node_modules/**/*",
                "out/**/*"
            ],
            "initialCopy": true,
            "initialCopyReverse": false,
            "deleteEnabled": true
        }
    ],
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
      {
        "before": ["j", "j"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<C-n>"],
        "commands": [":nohl"]
      }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false
    },
    "python.formatting.provider": "autopep8"
}